<p id="notice"><%= notice %></p>

<h1>Listing Teacher Generating Themes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_generating_themes.each do |teacher_generating_theme| %>
      <tr>
        <td><%= link_to 'Show', teacher_generating_theme %></td>
        <td><%= link_to 'Edit', edit_teacher_generating_theme_path(teacher_generating_theme) %></td>
        <td><%= link_to 'Destroy', teacher_generating_theme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher generating theme', new_teacher_generating_theme_path %>
